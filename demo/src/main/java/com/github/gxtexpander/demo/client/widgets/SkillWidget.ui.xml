<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder" xmlns:g="urn:import:com.google.gwt.user.client.ui">
  <ui:style>
  .clickable {
    cursor:pointer;
  }

  </ui:style>

  <ui:with type="com.github.gxtexpander.demo.client.DemoBundle" field="bundle"></ui:with>

  <g:FlowPanel>
    <g:InlineLabel ui:field="skillText"></g:InlineLabel>
    <g:Image ui:field="removeButton" resource="{bundle.remove}" styleName="{style.clickable}"></g:Image>
  </g:FlowPanel>

</ui:UiBinder>
